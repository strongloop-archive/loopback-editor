{
  "name": "loopback-editor",
  "version": "0.0.0",
  "description": "A web-based frontend for building and maintaining LoopBack Projects.",
  "scripts": {
    "test": "karma start karma.conf.js",
    "test-ci": "karma start karma.conf.js --reporters dots,junit --single-run",
    "blanket": {
      "pattern": "//^((?!(node_modules|test)).)*$/"
    },
    "coverage": "mocha -r blanket -R html-cov > coverage_loopback-editor.html"
  },
  "repository": "",
  "author": "",
  "dependencies": {
    "debug": "latest",
    "loopback-project-manager": "git+ssh://git@github.com:strongloop/loopback-workspace.git",
    "optimist": "~0.5.2",
    "st": "~0.2.1"
  },
  "devDependencies": {
    "karma": "~0.8.7",
    "mocha": "~1.8.2",
    "blanket": "latest"
  }
}
