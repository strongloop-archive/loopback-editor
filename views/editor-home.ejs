<% layout('layouts/editor-layout') %>
<% stylesheet('/css/editor-home.css') %>

<h2>Getting Started</h2>

<ul class="guide-links">
  <% generators.forEach(function(g) { %>
    <li>
      <a href="<%- g.path %>"><i class="icon-chevron-right"></i> <%= g.label %></a>
      <p><%= g.description %></p>
    </li>
  <% }); %>
</ul>